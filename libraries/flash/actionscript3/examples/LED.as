package {	import flash.display.Sprite;	import flash.events.Event;	import flash.events.MouseEvent;	import gainer.*;	public class LED extends Sprite {		private var _gio:Gainer;		private var _sqr:Sprite;		public function LED() {			// Gainerのインスタンスを生成する			_gio = new Gainer("localhost", 2000, Gainer.MODE1, true);			// I/OモジュールのLEDをコントロールするための簡易ボタンを生成			_sqr = new Sprite();			_sqr.graphics.beginFill(0x000000);			_sqr.graphics.drawRect(100, 60, 120, 120);			_sqr.graphics.endFill();			this.addChild(_sqr);			// I/Oモジュールの準備が完了した時に呼ばれるイベントハンドラをセット			_gio.onReady = _start;		}		// I/Oモジュールの準備が完了したら以下を実行		private function _start():void {			// ボタンをクリックできることを示すためにカーソルを変更			_sqr.buttonMode = true;			// ボタンに対してイベントリスナを設定			_sqr.addEventListener(MouseEvent.MOUSE_DOWN, _onPress);			_sqr.addEventListener(MouseEvent.MOUSE_UP, _onRelease);		}		private function _onPress(e:Event):void {			_gio.turnOnLED();		}		private function _onRelease(e:Event):void {			_gio.turnOffLED();		}	}}