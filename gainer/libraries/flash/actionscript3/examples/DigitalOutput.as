package {	import flash.display.Sprite;	import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.text.*;	import gainer.*;	public class DigitalOutput extends Sprite {		private var _gnr:Gainer;		private var dArray:Array = [false, false, false, false];		public function DigitalOutput() {			// Gainerのインスタンスを生成する			_gnr = new Gainer("localhost", 2000, Gainer.MODE1, true);			// I/Oモジュールの準備が完了した時に呼ばれるイベントハンドラをセット			_gnr.onReady = function() {				_start();			};		}		// I/Oモジュールの準備が完了したら以下を実行		private function _start():void {			// それぞれのテキストボタンに対して以下を実行			for (var i:int = 0; i < 4; i++) {				// MOUSE_UPイベントに対してイベントリスナをセット				this["d" + i].addEventListener(MouseEvent.MOUSE_UP, _updateOutputs);								// ボタンであることを示すためにカーソルを変更				this["d" + i].buttonMode = true;			}		}		private function _updateOutputs(e:Event):void {			if (e.target.currentFrame == 1) {				e.target.gotoAndStop(2);			} else {				e.target.gotoAndStop(1);			}			dArray[e.target.name.substr(1, 1)] = !dArray[e.target.name.substr(1, 1)];			_gnr.digitalOutput(dArray);		}	}}